class CampSite extends React.Component {
  constructor(props) {
    super(props);
  }
  render() {
    return (
      <div>
        <Camper  />
      </div>
    );
  }
};
// change code below this line
const Camper = props => {
  Camper.propTypes = { name: PropTypes.string.isRequired}
  Camper.defaultProps = {
  name: 'CamperBot'
};
  return (
 <div><h1>My First Camp!</h1><p>{props.name}</p></div>
  );
  }

  
